<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane onMouseEntered="#init" prefHeight="400.0" prefWidth="600.0" stylesheets="@../CSS/Style.CSS" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Sample.View.EndGamePageController">
   <children>
      <Rectangle id="endGameBackground" fx:id="background" arcHeight="5.0" arcWidth="5.0" fill="#f8f8f8" height="600.0" stroke="BLACK" strokeType="INSIDE" width="900.0" />
      <Label alignment="CENTER" layoutX="372.0" layoutY="283.0" prefHeight="35.0" prefWidth="75.0" text="Score: ">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="backToMenu" layoutX="344.0" layoutY="434.0" mnemonicParsing="false" onMouseClicked="#backToMenu" prefHeight="50.0" prefWidth="212.0" text="Back to main menu">
         <font>
            <Font size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label fx:id="score" alignment="CENTER" layoutX="450.0" layoutY="283.0" prefHeight="35.0" prefWidth="94.0">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="time" layoutX="397.0" layoutY="318.0" prefHeight="35.0" prefWidth="169.0">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Rectangle fx:id="state" arcHeight="5.0" arcWidth="5.0" blendMode="MULTIPLY" fill="#1f93ff" height="84.0" layoutX="327.0" layoutY="192.0" stroke="BLACK" strokeType="INSIDE" width="246.0" />
      <Button fx:id="newGame" layoutX="344.0" layoutY="362.0" mnemonicParsing="false" onMouseClicked="#newGame" prefHeight="50.0" prefWidth="212.0" text="Start new game">
         <font>
            <Font size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Rectangle fx:id="progress" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="40.0" layoutX="350.0" layoutY="232.0" stroke="BLACK" strokeType="INSIDE" visible="false" width="200.0" />
      <Rectangle fx:id="progressBar" arcHeight="5.0" arcWidth="5.0" fill="#ff1f1f" height="40.0" layoutX="350.0" layoutY="232.0" stroke="BLACK" strokeType="INSIDE" visible="false" width="200.0" />
   </children>
</AnchorPane>
